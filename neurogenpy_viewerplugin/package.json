{
  "name": "neurogenpy-viewer-plugin",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run theme && npm run theme:dark && rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -p 8000",
    "theme": "npm run theme:light && npm run theme:dark",
    "theme:light": "smui-theme compile public/build/smui.css -i src/theme",
    "theme:dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@smui-extra/autocomplete": "^6.0.0-beta.16",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/chips": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/select": "^6.0.0-beta.16",
    "@smui/switch": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@tsconfig/svelte": "^3.0.0",
    "graphology": "^0.24.1",
    "graphology-gexf": "^0.10.2",
    "graphology-types": "^0.24.4",
    "sigma": "^2.3.0",
    "smui-theme": "6.0.0-beta.16",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "graphology-layout": "^0.6.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
